package foo

import (
	"testing"

	//"github.com/MYORG/MYPKG/internal/FOO"
)

// TestFoo asserts that...
func TestFoo(t *testing.T) {
  good := "good"
  bad := "bad"
  if good != bad {
    t.Errorf("failed to function: want: %s, got: %s", good, bad)
  }
}
